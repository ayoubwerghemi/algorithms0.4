ALGORITHM insertion_sort
VAR
    n:INTEGER
    arr: ARRAY_OF INTEGER[n];
BEGIN
    read(n)
    FOR i FROM 0 TO n-1   DO
       read(arr[i])
    END_FOR
    FOR i FROM min TO max STEP step  DO
      var j=i-1 ;
      var current:=arr[i]
      WHILE (j>=0 AND current<arr[i]) DO
          arr[j+1]= arr [j]
          j:=j-1
      END_WHILE
      arr[j+1]= current
    END_FOR
    write(arr)
END
